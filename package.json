{
  "name": "embedza",
  "version": "1.2.2",
  "description": "Create HTML snippets/embeds from URLs using info from oEmbed, Open Graph, meta tags.",
  "keywords": [
    "youtube",
    "vimeo",
    "oembed",
    "opengraph"
  ],
  "repository": "nodeca/embedza",
  "license": "MIT",
  "files": [
    "assets/",
    "config/generic.yml",
    "lib/",
    "support/",
    "index.js"
  ],
  "dependencies": {
    "any-promise": "^1.3.0",
    "async": "^1.3.0",
    "cheerio": "^0.20.0",
    "debug": "^2.2.0",
    "js-yaml": "^3.3.1",
    "lodash": "^4.0.0",
    "probe-image-size": "^2.0.1",
    "request": "^2.58.0",
    "require-all": "^2.0.0"
  },
  "devDependencies": {
    "coveralls": "^2.11.9",
    "eslint": "~3.0.1",
    "express": "^4.13.1",
    "istanbul": "^0.4.1",
    "jade": "^1.11.0",
    "mocha": "^2.2.5",
    "nock": "^8.0.0"
  },
  "scripts": {
    "lint": "./node_modules/.bin/eslint .",
    "test": "npm run lint && ./node_modules/.bin/mocha",
    "coverage": "rm -rf coverage && ./node_modules/.bin/istanbul cover node_modules/.bin/_mocha",
    "report-coveralls": "./node_modules/.bin/istanbul cover ./node_modules/mocha/bin/_mocha --report lcovonly -- -R spec && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "install": "node support/install",
    "start": "node support/server"
  }
}
